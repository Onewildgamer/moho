<?xml version="1.0" encoding="UTF-8"?>
<sip-app>
  <app-name>MohoSampleCPA</app-name>

  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>com.voxeo.moho.sip.SIPController</servlet-class>
    <init-param>
      <param-name>ApplicationClass</param-name>
      <param-value>com.voxeo.moho.sample.CPA</param-value>
    </init-param>
    <init-param>
      <param-name>To</param-name>
      <param-value>sip:to@address:port</param-value>
    </init-param>
    <init-param>
      <param-name>time</param-name>
      <param-value>10000</param-value>
    </init-param>
    <init-param>
      <param-name>mediaDialectClass</param-name>
      <param-value>com.voxeo.prism.moho.PrismDialect</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-selection>
    <main-servlet>Controller</main-servlet>
  </servlet-selection>
</sip-app>